import cv2
import numpy as np

img = cv2.imread(r"C:\Users\yasaswini kuchi\OneDrive\Pictures\Screenshots\Screenshot 2026-02-11 095427.png", 0)

kernel = np.array([[-1,-1,-1],
                   [-1, 8,-1],
                   [-1,-1,-1]])

out = cv2.filter2D(img, -1, kernel)
cv2.imshow("Boundary", out)
cv2.waitKey(0)
cv2.destroyAllWindows()
